{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "es6",
    "strict": true,
    "jsx": "react", // Enable JSX support
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/AuthScreen.tjs", "app/HomePage.js", "app/server.js", "app/server.js", "app/Callback.js"  ]
}
